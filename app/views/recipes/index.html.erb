<h1>WellBee ğŸ</h1>
<p id="notice"><%= notice %></p>
<h2>Recipes</h2>


<table>
  


  <tbody>
    <% @recipes.each do |recipe| %>
      <ul>
        <li><%= recipe.recipe_title %></li>
        <li><%= recipe.recipe_content %></li>
        <li><%= recipe.user.first_name %></li>
        <li><%= link_to 'Show', recipe %></li>
        <li><%= link_to 'Edit', edit_recipe_path(recipe) %></li>
        <li><%= link_to 'New Recipe', new_recipe_path(recipe) %></li>
        <li><%= link_to 'Delete', recipe, method: :delete, data: { confirm: 'Are you sure?' } %></li>
      </ul>
    <% end %>
  </tbody>
</table>

